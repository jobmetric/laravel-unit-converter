{
    "name": "jobmetric/laravel-unit-converter",
    "description": "This is a unit management package for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "unit",
        "management",
        "converter",
        "measurement",
        "conversion"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-unit-converter",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.37",
        "jobmetric/laravel-translation": "^3.7",
        "jobmetric/laravel-event-system": "^2.8",
        "jobmetric/media": "^1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\UnitConverter\\": "src/",
            "JobMetric\\UnitConverter\\Factories\\": "database/factories/",
            "JobMetric\\UnitConverter\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\UnitConverter\\UnitConverterServiceProvider"
            ],
            "aliases": {
                "UnitConverter": "JobMetric\\UnitConverter\\Facades\\UnitConverter"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-unit-converter/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-unit-converter"
    }
}
